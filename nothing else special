.text
    main:
        li $t0, 5         # Load immediate value 5 into register $t0
        li $t1, 7         # Load immediate value 7 into register $t1
        add $t2, $t0, $t1 # Add values in $t0 and $t1, store result in $t2

        # Exit program
        li $v0, 10        # Load the exit system call code
        syscall           # Make system call to exit
